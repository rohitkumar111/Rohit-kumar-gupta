<script type="text/javascript">
      $(function(){
    $('select').change(function(){
        if($(this).attr('id') == 'box_g1' && $(this).val() == 'Default'){
            $('select').not(this).prop('disabled', true).val('Disabled');
        } else {
            $('select').not(this).removeProp('disabled');
            $('select option').removeProp('disabled');
            $('select').each(function(){
                var val = $(this).val();
                if(val != 'Default' || val != 'Disabled'){
                    $('select option[value="'+val+'"]').not(this).prop('disabled', true);
                }
            });
        }
    });
});
// Get the modal
var modal = document.getElementById('myModal');

// Get the button that opens the modal
var btn = document.getElementById("myBtn");

// Get the <span> element that closes the modal
var span = document.getElementsByClassName("close")[0];

// When the user clicks the button, open the modal 
btn.onclick = function() {
    modal.style.display = "block";
  var tryelm = [];
  for(var i=1;i<=13;i++){
    var idname = "box_g";
    var name = idname.concat(i);
    var e = document.getElementById(name);
    var value = e.options[e.selectedIndex].text;
    tryelm.push(value);
  }
  var researcher = [2,5,3,11,13,6,7,8,9,4,1,12,10];
  var industrial = [6,2,7,9,13,10,4,8,12,3,1,11,5];
  var totalres=0;
  var totalind=0;
  for(var i=0;i<13;i++){
  totalres+=Math.abs(tryelm[i]-researcher[i]);
  totalind+=Math.abs(tryelm[i]-industrial[i]);
  }
	if(totalres>totalind){
		document.getElementById("puttxt").innerHTML = "You seem to think more like Industry Professional";
	}else if(totalres<totalind){
		document.getElementById("puttxt").innerHTML = "You seem to think more like Researcher" ;
	
	}else if(totalres==totalind){
		document.getElementById("puttxt").innerHTML = "wow you seem to think like both Researcher and Industry Professional" ;
	}
	
}
// When the user clicks on <span> (x), close the modal
span.onclick = function() {
    modal.style.display = "none";
}
// When the user clicks anywhere outside of the modal, close it
window.onclick = function(event) {
    if (event.target == modal) {
        modal.style.display = "none";
    }
}
</script>

